{% extends 'base.html.twig' %}

{% block title %}TrackEntity{% endblock %}

{% block body %}
    <h1>TrackEntity</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ track_entity.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ track_entity.title }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ track_entity.duration }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_web_track_index') }}">back to list</a>

    {% if is_granted('ROLE_ADMIN') %}
        <a href="{{ path('app_web_track_edit', {'id': track_entity.id}) }}">edit</a>
        {{ include('web/track/_delete_form.html.twig') }}
    {% endif %}

{% endblock %}
